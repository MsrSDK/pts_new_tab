{
  "manifest_version": 3,
  "name": "PTS New Tab",
  "version": "1.0.0",
  "description": "PTSに新しいタブで開く機能を追加する",
  "devtools_page": "devtools.html",
  "content_scripts": [
    {
      "matches": [
        "https://agri-pts.optim.co.jp/*",
        "https://agri-pts-test.optim-test.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "tabs"
  ]
}